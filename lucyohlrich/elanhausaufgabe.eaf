            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a18" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>und siebenundneunzig da war schon ein kleines Mädchen da,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a19" ANNOTATION_REF="a9">

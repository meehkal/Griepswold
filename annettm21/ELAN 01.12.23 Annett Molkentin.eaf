    <TIER LINGUISTIC_TYPE_REF="orthT" PARENT_REF="ref" TIER_ID="orth">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a13" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Ich komme von Schöneiche und werde euch mal ein bisschen erzählen</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a14" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>1896 da hab ich mich verheitratet und 97' da war schon ein kleines Mädchen da</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a15" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Und in vier Jahren ein kleiner Jung</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a16" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Sie sind ja nun schon alle groß und sind verheiratet und haben schon selber Kinder</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a17" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>Und mein Mann war ein ganzes Jahr krank</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a18" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Aber der schlupft schon unter der Erde</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>Und mein Schiegervater, das war ein ganz anderer Kerl gewesen</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>Der 104 Jahre alt geworden</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>Und war in seinem Leben fast gar nicht krank</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>Und wo er eingeseget war</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>Da ist ihnen der gesamte Hof abgebrannt</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>Da musste er zu den anderen Dörfern fahren, um zu betteln</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER LINGUISTIC_TYPE_REF="ft-engT" PARENT_REF="orth" TIER_ID="ft-eng">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a19" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>I'm from Schöneiche and will tell you something</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a20" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>I got married in 1896 and in 97' we had a little girl</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>And four years later a little boy</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>They are all grown up now and have kids of their own</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>And my husband was sick for a whole year</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>But he is already asleep under the ground</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>And my father in law was a very different guy</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>He lived until the age of 104</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE>And was never really sick </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>And after his conformation happend</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE>Their whole farm burned down</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>So he had to drive to the other villages to beg</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
